#include<stdio.h>
int main()
{
	int a[3] [3],x,y;
	printf("Enter Matrix Elements : \n");
	for(x=0;x<3;x++)
	{
		for(y=0;y<3;y++)
		{
			printf("Enter the elements[%d][%d]",x,y;
			scanf("%d",&a[x][y]);
		}
	}
	printf("Matrix is\n");
	for(x=0;x<3;x++)
	{
		for(y=0;y<3;y++)
		{
			printf("%3d",a[x][y]);
		}
	}
	getch();
}
